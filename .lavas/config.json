{
    "buildVersion": 1557301166355,
    "build": {
        "ssr": false,
        "publicPath": "/cloudmusic/",
        "compress": false,
        "cssExtract": false
    },
    "middleware": {
        "all": [],
        "server": [],
        "client": []
    },
    "router": {
        "mode": "history",
        "base": "/cloudmusic/",
        "pageTransition": {
            "type": "fade",
            "transitionClass": "fade"
        },
        "rewrite": [
            {
                "from": "/recommend",
                "to": "/recommend/index"
            },
            {
                "from": "/singer",
                "to": "/singer/index"
            },
            {
                "from": "/rank",
                "to": "/rank/index"
            },
            {
                "from": "/search",
                "to": "/search/index"
            }
        ]
    },
    "errorHandler": {
        "defaultErrorMessage": "Internal Server Error",
        "showRealErrorMessage": false,
        "errorPath": "/error"
    },
    "serviceWorker": {
        "enable": true,
        "swDest": "/Users/yangyang/kou-exercise/cloudmusic/dist/service-worker.js"
    },
    "skeleton": {
        "enable": true,
        "asyncCSS": true
    }
}